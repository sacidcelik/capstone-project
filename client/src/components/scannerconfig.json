{
  "inputStream": {
    "type": "LiveStream",
    "constraints": {
      "width": { "min": 300 },
      "height": { "min": 200 },
      "facingMode": "environment",
      "aspectRatio": { "min": 1.5 }
    },
    "area": {
            "top": "0%", 
            "right": "0%", 
            "left": "0%",
            "bottom": "0%"
          }
  },
  "locator": {
    "patchSize": "medium",
    "halfSample": true
  },
  "numOfWorkers": 2,
  "frequency": 10,
  "decoder": {
    "readers": ["ean_reader"]
  },
  "locate": true
}
